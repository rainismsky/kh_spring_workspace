	private String eid; //id 
	private String pwd; //pw
	private String e_name; //이름
	private String e_birth; //생년월일
	private String e_num; //대표 휴대폰
	private String e_email; //이메일
	private String ent_name; //업체명
	private String ent_num; //업체 전화번호
	private String ent_address; //업체 주소 
	private String ent_zipcode; //우편 번호
	private String ent_type; //업종
	private String ent_regnum; //사업자 등록 번호
	private String ent_open; //open time
	private String ent_close; //close time
	private String fixed_closure; //정기 휴무일
	private String theme; //테마

}
